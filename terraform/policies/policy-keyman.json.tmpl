{
  "Version": "2012-10-17",
  "Statement": [
        {
            "Action": [
                "iam:DeleteAccessKey",
                "iam:CreateAccessKey",
                "iam:UpdateAccessKey",
                "iam:ListUsers",
                "iam:GetUser",
                "iam:ListAccessKeys"
            ],
            "Effect": "Allow",
            "Resource": "arn:aws:iam::${account_id}:user/*"
        },
        {
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Effect": "Allow",
            "Resource": "arn:aws:logs:${region}:${account_id}:log-group:${cw_log_group}:*"
        }
    ]
}